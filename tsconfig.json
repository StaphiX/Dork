{
    "compilerOptions": {
      //"allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "target": "ES6",
      "module": "commonjs",
      "moduleResolution": "node",
      "noResolve": false,
      //"noImplicitAny": false,
      "removeComments": true,
      "sourceMap": true,
      "allowJs": true,
      "jsx": "react",
      //"rootDir": "src/",
      "outDir": "./built",
      "baseUrl": "src/",
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
      "./node_modules/**/*",
      "./dist",
      "./src/server"
    ]
  }